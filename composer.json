{
    "name": "adi3890/phpcs-common-rules",
    "description": "Organization-wide PHP CodeSniffer security standards and common rules",
    "type": "phpcodesniffer-standard",
    "license": "MIT",
    "authors": [
        {
            "name": "Aditya Jain",
            "email": "adi3890@users.noreply.github.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Adi3890\\PHPCSCommonRules\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": [
        "phpcs",
        "security",
        "code-quality",
        "standards",
        "php"
    ],
    "homepage": "https://github.com/adi3890/phpcs-common-rules",
    "support": {
        "issues": "https://github.com/adi3890/phpcs-common-rules/issues",
        "source": "https://github.com/adi3890/phpcs-common-rules"
    }
}
